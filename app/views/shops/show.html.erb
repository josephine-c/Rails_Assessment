<p id="notice"><%= notice %></p>
<h3><%= @shop.name %></h3>
<h5><%= @shop.description %></h5>
<ul>
    <% @products.each do |product| %>
        <li><%= product.name %></li>
        <li><%= product.description %></li>
        <li><%= product.stock %></li>
        <li><%= product.size %></li>
        <li><%= product.price %></li>
    <% end %>
</ul>
<%= link_to 'Edit', edit_shop_path(@shop) %> 
<%= link_to 'Back', shops_path %>
